<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="cs4720.cs.virginia.edu.updatedList">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="cs4720.cs.virginia.edu.updatedList.BucketListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".AddItemActivity"
            android:label="Add New Item">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cs4720.cs.virginia.edu.updatedList.BucketListActivity" />
        </activity>
        <activity
            android:name=".EditItemActivity"
            android:label="Edit Item">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="cs4720.cs.virginia.edu.updatedList.BucketListActivity" />
        </activity>
    </application>

</manifest>